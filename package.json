{
  "name": "ratiomaxx-root",
  "private": true,
  "scripts": {
    "dev:ui": "npm --prefix apps/ui run dev",
    "dev:api": "npm --prefix apps/api run dev",
    "docker:ml": "docker compose up --build ml-service",
    "dev:all": "concurrently -k -n UI,API,ML -c green,cyan,yellow \"npm run dev:ui\" \"npm run dev:api\" \"npm run docker:ml\"",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}
